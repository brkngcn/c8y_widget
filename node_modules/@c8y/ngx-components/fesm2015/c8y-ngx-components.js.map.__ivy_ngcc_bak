om '@angular/core';\nimport { ApplicationRemotePlugins, IApplication, IIdentified } from '@c8y/client';\nimport { ApplicationService } from '@c8y/ngx-components/api';\nimport { AppStateService } from '../common/ui-state.service';\n\ntype Application = IIdentified | IApplication | number | string;\ntype Package = Application;\n\n@Injectable()\nexport class PluginsService {\n  constructor(\n    private applicationService: ApplicationService,\n    private appState: AppStateService\n  ) {}\n\n  async listPackages() {\n    const { data } = await this.applicationService.list({ pageSize: 2000 });\n    return data.filter((app: any) => app.exports || (app.manifest && app.manifest.exports));\n  }\n\n  async listVersions(forPackage: Package) {\n    const { data } = await this.applicationService.binary(forPackage).listPlugins();\n    return data\n      .map(plugin => ({ ...plugin.pluginPackage, version: plugin.pluginName }))\n      .filter(plugin => plugin.exports);\n  }\n\n  async listInstalled(forApp: Application, flat = true) {\n    const app = await this.getApplication(forApp);\n    const c8yJson = await this.getCumulocityJsonFile(app);\n    if (flat) {\n      let flatList = [];\n      for (const key in c8yJson.imports) {\n        if (c8yJson.imports.hasOwnProperty(key)) {\n          flatList = [\n            ...flatList,\n            ...c8yJson.imports[key].map(imp =>\n              /@/.test(key) ? `${key}/${imp}` : `${key}@latest/${imp}`\n            )\n          ];\n        }\n      }\n      return flatList;\n    }\n    return c8yJson.imports;\n  }\n\n  async addByName(addTo: Application, pluginName: string) {\n    const name = pluginName.split('//').pop();\n    const contextPath = pluginName.split('//').shift();\n    const pkg = {\n      contextPath\n    };\n    return this.add(addTo, pkg, name);\n  }\n\n  async removeByName(removeFrom: Application, pluginName: string) {\n    const name = pluginName.split('/').pop();\n    const contextPath = /@latest/.test(pluginName)\n      ? pluginName.split('@').shift()\n      : pluginName.split('/').shift();\n\n    const pkg = {\n      contextPath\n    };\n    return this.remove(removeFrom, pkg, name);\n  }\n\n  async add(addToApp: Application, fromPackage: Package, name: string) {\n    return this.addOrRemove(addToApp, fromPackage, name, true);\n  }\n\n  async remove(removeFromApp: Application, fromPackage: Package, name: string) {\n    return this.addOrRemove(removeFromApp, fromPackage, name, false);\n  }\n\n  async updateRemotesInCumulocityJson(app: IApplication, remotes: ApplicationRemotePlugins) {\n    const c8yJson = await this.getCumulocityJsonFile(app);\n    return this.applicationService.storeAppManifest(app, { ...c8yJson, remotes });\n  }\n\n  private async addOrRemove(addToApp: Application, fromPackage: Package, name: string, add = true) {\n    const application = await this.getApplication(addToApp);\n    const pkg = await this.getApplication(fromPackage);\n    const c8yJson = await this.getCumulocityJsonFile(application);\n\n    // TODO: Versions?!!\n    const pkgImport = new Set(c8yJson.imports[pkg.contextPath]);\n    if (add) {\n      pkgImport.add(name);\n    } else {\n      pkgImport.delete(name);\n    }\n    c8yJson.imports[pkg.contextPath] = Array.from(pkgImport);\n\n    await this.applicationService.storeAppManifest(application, c8yJson);\n    return c8yJson;\n  }\n\n  private async getCumulocityJsonFile(app: IApplication) {\n    const c8yJson = await this.applicationService.getAppManifest(app);\n    if (!c8yJson.imports) {\n      c8yJson.imports = {};\n    }\n    return c8yJson;\n  }\n\n  private async getApplication(app: Application): Promise<IApplication> {\n    if (typeof app !== 'number' && (app as IApplication).contextPath) {\n      return app as IApplication;\n    }\n    const { data } = await this.applicationService.detail(app);\n    return data;\n  }\n}\n","import { NgModule } from '@angular/core';\nimport { PluginsResolveService } from './plugins-resolve.service';\