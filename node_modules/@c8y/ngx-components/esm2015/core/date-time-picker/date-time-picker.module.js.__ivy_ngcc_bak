import { NgModule } from '@angular/core';
import { ReactiveFormsModule } from '@angular/forms';
import { CommonModule as C8yCommonModule } from '../common/common.module';
import { FormsModule } from '@angular/forms';
import { CommonModule } from '@angular/common';
import { FormsModule as C8yFormsModule } from '../forms/index';
import { BsDatepickerModule } from 'ngx-bootstrap/datepicker';
import { DateTimePickerComponent } from './date-time-picker.component';
import { TimePickerModule } from '../time-picker/time-picker.module';
/**
 * The angular module definition for a date-time picker - it provides FormControl that handles date and time.
 * @exports DateTimePickerComponent
 */
export class DateTimePickerModule {
}
DateTimePickerModule.decorators = [
    { type: NgModule, args: [{
                imports: [
                    C8yFormsModule,
                    CommonModule,
                    FormsModule,
                    ReactiveFormsModule,
                    BsDatepickerModule.forRoot(),
                    C8yCommonModule,
                    TimePickerModule,
                ],
                exports: [DateTimePickerComponent],
                declarations: [DateTimePickerComponent]
            },] }
];
