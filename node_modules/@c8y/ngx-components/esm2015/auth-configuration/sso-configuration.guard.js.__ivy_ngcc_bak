import { Injectable } from '@angular/core';
import { SsoConfigurationService } from './sso-configuration.service';
export class SsoConfigurationGuard {
    constructor(ssoConfigurationService) {
        this.ssoConfigurationService = ssoConfigurationService;
    }
    canActivate() {
        return this.ssoConfigurationService.showSsoConfiguration$();
    }
}
SsoConfigurationGuard.decorators = [
    { type: Injectable }
];
SsoConfigurationGuard.ctorParameters = () => [
    { type: SsoConfigurationService }
];
