    var SearchActionComponent = /** @class */ (function () {
        function SearchActionComponent(c8yRouter, router) {
            this.c8yRouter = c8yRouter;
            this.router = router;
        }
        SearchActionComponent.prototype.onSearch = function (on) {
            this.navigate(['/assetsearch'], {
