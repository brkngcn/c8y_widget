                replaceUrl: true
            });
        };
        SearchActionComponent.prototype.onClick = function (mo) {
            this.router.navigateByUrl(this.c8yRouter.getHref(mo, '/'));
        };
        SearchActionComponent.prototype.navigate = function (commands, extras) {
            var _this = this;
            this.router
                .navigateByUrl('/', { skipLocationChange: true })
                .then(function () { return _this.router.navigate(commands, extras); });
        };
        return SearchActionComponent;
    }());
