import { InjectionToken, Injector } from '@angular/core';
import { Router } from '@angular/router';
import { ExtensionFactory, ExtensionPoint } from '@c8y/ngx-components';
import { Observable, Subject } from 'rxjs';
import { RegisterDeviceItem } from './RegisterDeviceItem';
export declare type RegisterDeviceExtension = RegisterDeviceItem | RegisterDeviceItem[] | ExtensionFactory<RegisterDeviceItem>;
export declare const HOOK_DEVICE_REGISTRATION: InjectionToken<RegisterDeviceExtension>;
export declare class RegisterDeviceExtensionService implements ExtensionPoint<RegisterDeviceExtension> {
    private injector;
    items$: Observable<RegisterDeviceItem[]>;
    factories: RegisterDeviceExtension[];
    refreshTrigger: Subject<any>;
    constructor(injector: Injector, router: Router);
    refresh(): void;
}
