import { DeviceRegistrationStatus } from '@c8y/client';
import { RealtimeService } from './realtime.service';
import { RealtimeSubjectService } from './realtime-subject.service';
export interface IRealtimeDeviceBootstrap {
    id: string;
    status: DeviceRegistrationStatus;
}
/**
 * A service for handling device bootstrap realtime notifications in RxJS fashion.
 */
export declare class DeviceBootstrapRealtimeService extends RealtimeService<IRealtimeDeviceBootstrap> {
    protected realtimeSubject: RealtimeSubjectService;
    constructor(realtimeSubject: RealtimeSubjectService);
    protected channel(): string;
}
