    "css-minimizer-webpack-plugin": "3.0.2",
    "express": "4.16.4",
    "fast-glob": "2.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "5.3.2",
    "http-proxy-middleware": "0.19.1",
    "imports-loader": "^0.8.0",
    "inquirer": "^8.1.2",
