    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.15.4",
    "@c8y/client": "1015.0.367",
    "JSONPath": "0.11.2",
    "angular-gettext-tools": "2.5.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-angularjs-annotate": "0.10.0",
